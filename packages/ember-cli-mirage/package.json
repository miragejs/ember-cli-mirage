{
  "name": "ember-cli-mirage",
  "version": "3.0.0-alpha.3",
  "description": "A client-side server to help you build, test and demo your Ember app",
  "keywords": [
    "ember-addon",
    "pretender",
    "prototype",
    "server",
    "testing"
  ],
  "homepage": "https://github.com/miragejs/ember-cli-mirage",
  "bugs": {
    "url": "https://github.com/miragejs/ember-cli-mirage/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/miragejs/ember-cli-mirage.git"
  },
  "license": "MIT",
  "author": "Sam Selikoff",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "test:types": "tsc --project types-tests"
  },
  "dependencies": {
    "@embroider/macros": "^1.10.0",
    "broccoli-file-creator": "^2.1.1",
    "broccoli-funnel": "^3.0.3",
    "broccoli-merge-trees": "^4.2.0",
    "ember-auto-import": "^2.6.1",
    "ember-cli-babel": "^7.26.11",
    "ember-get-config": "0.2.4 - 0.5.0 || ^1.0.0 || ^2.1.1",
    "ember-inflector": "^2.0.0 || ^3.0.0 || ^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.22.17",
    "@ember/jquery": "^2.0.0",
    "@ember/optional-features": "^2.0.0",
    "@ember/string": "^3.1.1",
    "@ember/test-helpers": "^2.9.3",
    "@ember-data/model": "~4.11.3",
    "@ember-data/store": "~4.11.3",
    "@ember-data/tracking": "~4.11.3",
    "@embroider/test-setup": "^2.1.1",
    "@faker-js/faker": "^6.3.1",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@types/qunit": "^2.19.4",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^8.0.1",
    "ember-cli": "~4.11.0",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-fastboot": "^4.1.1",
    "ember-cli-favicon": "^3.0.0",
    "ember-cli-htmlbars": "^6.2.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-moment-shim": "^3.8.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-data": "~4.11.3",
    "ember-fetch": "^8.1.2",
    "ember-load-initializers": "^2.1.2",
    "ember-moment": "^9.0.1",
    "ember-page-title": "^8.0.0",
    "ember-qunit": "^6.2.0",
    "ember-resolver": "^10.0.0",
    "ember-source": "~4.11.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^5.11.2",
    "ember-try": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-ember": "^11.4.9",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qunit": "^7.3.4",
    "fastboot": "^3.3.0",
    "js-yaml": "^4.0.0",
    "loader.js": "^4.7.0",
    "miragejs": "^0.1.47",
    "prettier": "^2.8.7",
    "qunit": "^2.19.4",
    "qunit-dom": "^2.0.0",
    "typescript": "^4.5.5",
    "webpack": "^5.75.0"
  },
  "peerDependencies": {
    "@ember/test-helpers": "*",
    "@ember-data/model": "*",
    "ember-data": "*",
    "ember-qunit": "*",
    "ember-source": "^3.28.0 || ^4.0.0 || ^5.0.0",
    "miragejs": "^0.1.43"
  },
  "peerDependenciesMeta": {
    "@ember/test-helpers": {
      "optional": true
    },
    "@ember-data/model": {
      "optional": true
    },
    "ember-data": {
      "optional": true
    },
    "ember-qunit": {
      "optional": true
    }
  },
  "engines": {
    "node": "16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "before": [
      "ember-cli-babel"
    ],
    "after": [
      "ember-qunit"
    ]
  },
  "volta": {
    "extends": "../../package.json"
  }
}
